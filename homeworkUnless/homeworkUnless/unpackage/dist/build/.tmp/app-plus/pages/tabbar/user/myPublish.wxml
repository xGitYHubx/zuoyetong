<view><view class="cu-card dynamic"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['list','',index]]]]]]]}}" class="cu-item shadow" bindtap="__e"><view class="cu-list menu-avatar"><view class="cu-item"><view class="cu-avatar round lg" style="{{('background-image:url('+item.avatarUrl+');')}}"></view><view class="content flex-sub"><view>{{item.name||item.account}}</view><view class="text-gray text-sm flex justify-between">{{''+item.createTime+''}}</view></view></view></view><view class="text-content">{{''+item.descText+''}}</view><block wx:if="{{item.firstUrl}}"><view class="{{['grid flex-sub padding-lr',isCard?'col-3 grid-square':'col-1']}}"><image src="{{item.firstUrl||'#'}}" mode="widthFix"></image></view></block><view class="text-orange cuIcon-coin text-right padding">{{item.reward+''}}</view></view></block><block wx:if="{{isLoading}}"><view class="rw-page-loading">{{''+loadingMsg+''}}</view></block></view></view>